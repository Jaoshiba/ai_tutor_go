ascript"},"iframe":null,"mixtapeMetadata":null},"Paragraph:f5e6a177365e_62":{"__typename":"Paragraph","id":"f5e6a177365e_62","name":"bd85","type":"P","href":null,"layout":null,"metadata":null,"text":"In this case, the .find method searches through the users array and returns the first user object where it isid is equal to 2 or (===2)","hasDropCap":null,"dropCapImage":null,"markups":[{"__typename":"Markup","type":"CODE","start":18,"end":23,"href":null,"anchorType":null,"userId":null,"linkMetadata":null},{"__typename":"Markup","type":"CODE","start":52,"end":57,"href":null,"anchorType":null,"userId":null,"linkMetadata":null},{"__typename":"Markup","type":"CODE","start":109,"end":135,"href":null,"anchorType":null,"userId":null,"linkMetadata":null}],"codeBlockMetadata":null,"iframe":null,"mixtapeMetadata":null},"Paragraph:f5e6a177365e_63":{"__typename":"Paragraph","id":"f5e6a177365e_63","name":"d91e","type":"P","href":null,"layout":null,"metadata":null,"text":"Classwork implementation","hasDropCap":null,"dropCapImage":null,"markups":[],"codeBlockMetadata":null,"iframe":null,"mixtapeMetadata":null},"Paragraph:f5e6a177365e_64":{"__typename":"Paragraph","id":"f5e6a177365e_64","name":"1420","type":"H3","href":null,"layout":null,"metadata":null,"text":"To-Do List Implementation","hasDropCap":null,"dropCapImage":null,"markups":[],"codeBlockMetadata":null,"iframe":null,"mixtapeMetadata":null},"Paragraph:f5e6a177365e_65":{"__typename":"Paragraph","id":"f5e6a177365e_65","name":"0a6e","type":"PRE","href":null,"layout":null,"metadata":null,"text":"\u002F\u002F Simple To-Do List with CRUD functionality in JavaScript\n\nconst todoList = [];\n\n\u002F\u002F Function to create (add) a task\nfunction addTask(taskName) {\n    if (taskName) {\n        const task = { name: taskName, completed: false };\n        todoList.push(task);\n        console.log(`Task added: \"${taskName}\"`);\n    } else {\n        console.log(\"Please provide a valid task.\");\n    }\n}\n\n\u002F\u002F Function to read (view) tasks\nfunction viewTasks() {\n    if (todoList.length === 0) {\n        console.log(\"No tasks in the to-do list.\");\n    } else {\n        console.log(\"To-Do List:\");\n        todoList.forEach((task, id) =\u003E {\n            console.log(`${id + 1}: ${task.name} [${task.completed ? \"Completed\" : \"Pending\"}]`);\n        });\n    }\n}\n\n\u002F\u002F Function to update a task using .find()\nfunction updateTask(oldTaskName, newTaskName) {\n    const task = todoList.find(task =\u003E task.name === oldTaskName);\n    if (task && newTaskName) {\n        task.name = newTaskName; \u002F\u002F Update the task name\n        console.log(`Task updated from \"${oldTaskName}\" to \"${newTaskName}\"`);\n    } else {\n        console.log(\"Task not found or new task name is empty.\");\n    }\n}\n\n\u002F\u002F Function to mark a task as completed\nfunction completeTask(taskName) {\n    const task = todoList.find(task =\u003E task.name === taskName);\n    if (task) {\n        task.completed = true; \u002F\u002F Mark the task as completed\n        console.log(`Task \"${taskName}\" marked as completed.`);\n    } else {\n        console.log(\"Task not found.\");\n    }\n}\n\n\u002F\u002F Function to delete a task using .filter()\nfunction removeTask(taskName) {\n    const initialLength = todoList.length; \u002F\u002F Store the initial length of the todoList\n    todoList = todoList.filter(task =\u003E task.name !== taskName); \u002F\u002F Filter out the task to be removed\n\n    if (todoList.length \u003C initialLength) {\n        console.log(`Task removed: \"${taskName}\"`);\n    } else {\n        console.log(\"Task not found.\");\n    }\n}\n\n\u002F\u002F Example usage\naddTask(\"Buy groceries\");\naddTask(\"Walk the dog\");\nviewTasks(); \u002F\u002F Read\nupdateTask(\"Buy groceries\", \"Buy groceries and cook dinner\"); \u002F\u002F Update\ncompleteTask(\"Walk the dog\"); \u002F\u002F Mark as completed\nviewTasks(); \u002F\u002F Read\nremoveTask(\"Buy groceries\"); \u002F\u002F Delete\nviewTasks(); \u002F\u002F Read","hasDropCap":null,"dropCapImage":null,"markups":[],"codeBlockMetadata":{"__typename":"CodeBlockMetadata","mode":"AUTO","lang":"javascript"},"iframe":null,"mixtapeMetadata":null},"Paragraph:f5e6a177365e_66":{"__typename":"Paragraph","id":"f5e6a177365e_66","name":"2e4e","type":"H3","href":null,"layout":null,"metadata":null,"text":"Expected JSON Output Schema","hasDropCap":null,"dropCapImage":null,"markups":[],"codeBlockMetadata":null,"iframe":null,"mixtapeMetadata":null},"Paragraph:f5e6a177365e_67":{"__typename":"Paragraph","id":"f5e6a177365e_67","name":"83f2","type":"PRE","href":null,"layout":null,"metadata":null,"text":"{\n  \"todoList\": [\n    {\n      \"name\": \"Finish my assignment\",\n      \"completed\": true\n    },\n    {\n      \"name\": \"Prepare for the next Inventors Class\",\n      \"completed\": true\n    },\n    {\n      \"name\": \"Go to the bank,before 9pm\",\n      \"completed\": false\n    }\n  ]\n}","hasDropCap":null,"dropCapImage":null,"markups":[],"codeBlockMetadata":{"__typename":"CodeBlockMetadata","mode":"AUTO","lang":"json"},"iframe":null,"mixtapeMetadata":null},"Paragraph:f5e6a177365e_68":{"__typename":"Paragraph","id":"f5e6a177365e_68","name":"ca04","type":"H3","href":null,"layout":null,"metadata":null,"text":"Summary","hasDropCap":null,"dropCapImage":null,"markups":[],"codeBlockMetadata":null,"iframe":null,"mixtapeMetadata":null},"Paragraph:f5e6a177365e_69":{"__typename":"Paragraph","id":"f5e6a177365e_69","name":"d07b","type":"ULI","href":null,"layout":null,"metadata":null,"text":"Spread Operator: Expands elements of an array.","hasDropCap":null,"dropCapImage":null,"markups":[{"__typename":"Markup","type":"STRONG","start":0,"end":15,"href":null,"anchorType":null,"userId":null,"linkMetadata":null}],"codeBlockMetadata":null,"iframe":null,"mixtapeMetadata":null},"Paragraph:f5e6a177365e_70":{"__typename":"Paragraph","id":"f5e6a177365e_70","name":"2097","type":"ULI","href":null,"lay